{
    "name": "PageTitleChanger",
    "version": "0.1",
    "description": "",
    "permissions": ["<all_urls>"],
    "browser_action": {},
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["https://www.amazon.com/s/*"],
            "js": ["js/contentScripts/amazon.js"]
        },
        {
            "matches": ["https://www.amazon.com/*"],
            "js": ["js/contentScripts/amazonResult.js"]
        },
        {
            "matches": ["https://keep.google.com/*"],
            "js": ["js/contentScripts/general.js"],
            "myConfig": {
                "fragment": "#label/Todos",
                "title": "Todos"
            }
        }
    ],
    "background": {
        "scripts": ["js/background/background_listenForReload.js"],
        "persistent": false
    }
}
