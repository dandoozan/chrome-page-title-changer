{
    "name": "PageTitleChanger",
    "version": "0.1",
    "description": "",
    "permissions": ["tabs", "*://*/*"],
    "browser_action": {
        "default_icon": "images/icon.png"
    },
    "icons": {
        "16": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
    },
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["https://www.amazon.com/s/*"],
            "js": ["js/contentScripts/amazon.js"]
        },
        {
            "matches": ["https://www.amazon.com/*"],
            "js": ["js/contentScripts/amazonResult.js"]
        },
        {
            "matches": ["https://keep.google.com/*"],
            "js": ["js/contentScripts/gkeep.js"],
            "dpd_fragment": "#label/Todos",
            "dpd_title": "Todos"
        }
    ],
    "background": {
        "scripts": ["js/background/background_listenForReload.js"],
        "persistent": false
    }
}
