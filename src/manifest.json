{
    "name": "PageTitleChanger",
    "version": "0.1",
    "description": "",
    "permissions": [
        "tabs",
        "*://*/*"
    ],
    "icons": {
        "16": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.google.com; object-src 'self'",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["https://www.amazon.com/s/*"],
            "js": ["js/execScript_amazon.js"]
        },
        {
            "matches": ["https://www.amazon.com/*"],
            "js": ["js/execScript_amazonResult.js"]
        }
    ]
}
